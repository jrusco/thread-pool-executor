{
  "name": "Java Dev Container",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "extensions": [
    "vscjava.vscode-java-pack",
    "pivotal.vscode-spring-boot",
    "vscjava.vscode-maven"
  ],
  "forwardPorts": [8080],
  "postCreateCommand": "mvn clean install",
  "remoteUser": "vscode"
}
